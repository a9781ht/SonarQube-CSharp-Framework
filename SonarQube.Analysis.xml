<?xml version="1.0" encoding="utf-8" ?>
<SonarQubeAnalysisProperties  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.sonarsource.com/msbuild/integration/2015/1">
  <Property Name="sonar.qualitygate.wait">true</Property>
  <Property Name="sonar.sourceEncoding">UTF-8</Property>
  <!-- Test Execution Report: Trx format (Supports wildcard patterns) -->
  <Property Name="sonar.cs.vstest.reportsPaths">**/*.trx</Property>
  <!-- Code Coverage Report: SonarQube Generic format (Does NOT support wildcard patterns, must use specific path) -->
  <Property Name="sonar.coverageReportPaths">ClassLib/test/TestResults/SonarQube.xml</Property>
</SonarQubeAnalysisProperties>
